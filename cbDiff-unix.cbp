<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="cbDiff for Unix" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Unix">
				<Option output="../../../devel/share/codeblocks/plugins/cbDiff" prefix_auto="0" extension_auto="1" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option host_application="codeblocks" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="-Wall" />
					<Add option="-std=c++11" />
					<Add option="-fPIC" />
					<Add option="-g" />
					<Add option="`wx-config --version=2.8 --cflags`" />
					<Add option="-DEXPORT_FUNC" />
					<Add directory="../../../include" />
					<Add directory="../../../sdk/wxscintilla/include" />
					<Add directory="../../../include/tinyxml" />
					<Add directory="./dtl" />
				</Compiler>
				<Linker>
					<Add option="`wx-config --version=2.8 --libs`" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -j9 ../../../devel/share/codeblocks/cbDiff.zip manifest.xml" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
		</Build>
		<Unit filename="images/reload.h" />
